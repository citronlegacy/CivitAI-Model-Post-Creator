{
  "manifest_version": 3,
  "name": "CivitAI Model Creator",
  "version": "1.0.0",
  "description": "Create CivitAI models using your authenticated session",
  "permissions": [
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "https://civitai.com/*"
  ],
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
